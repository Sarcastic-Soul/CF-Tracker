{
  "manifest_version": 3,
  "name": "Codeforces Tracker",
  "version": "1.0",
  "description": "Track your friends' progress on Codeforces and gamify the experience.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "128": "assets/icon.png"
  }
}
